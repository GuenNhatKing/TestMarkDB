# Ignore Python cache files
__pycache__/
*.pyc
*.pyo

# Ignore virtual environments
venv/
env/
.venv/

# Ignore git and metadata files
.github/
.git/
.gitignore
.dockerignore

# Ignore log and cache files
*.log
*.sqlite3
*.db
*.pid

# Ignore collected static files
staticfiles/

